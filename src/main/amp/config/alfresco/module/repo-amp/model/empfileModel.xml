<?xml version="1.0" encoding="UTF-8"?>

<!-- Custom Model -->

<!-- Note: This model is pre-configured to load at startup of the Repository.  So, all custom -->
<!--       types and aspects added here will automatically be registered -->

<model name="empfm:empfileModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <!-- Optional meta-data about the model -->   
   <description>Custom Model</description>
   <author></author>
   <version>1.0</version>

   <imports>
   	  <!-- Import Alfresco Dictionary Definitions -->
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
      <!-- Import Alfresco Content Domain Model Definitions -->
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
   </imports>

   <!-- Introduction of new namespaces defined by this model -->
   <!-- NOTE: The following namespace custom.model should be changed to reflect your own namespace -->
   <namespaces>
      <namespace uri="custom.model" prefix="empfm"/>
   </namespaces>
      

  <types>   
 <type name="empfm:empdoc">
            <title>Employee Document</title>
            <parent>cm:content</parent>
            <properties>
        	<property name="empfm:EmployeeName">
        		<title>Employee Name</title>
        		<description>Name of the microarray project</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="empfm:EmployeeId">
        		<title>Employee Id</title>
        		<description>Employee Id</description>
        		<type>d:text</type>
        	</property>
        	<property name="empfm:UID">
        		<title>Unique File Identifier</title>
        		<description>Department Name</description>
        		<type>d:text</type>
                </property>              	
          </properties>
        </type>

 <type name="empfm:resume">
            <title>Resume</title>
            <parent>empfm:empdoc</parent>
            <properties>
               <property name="empfm:DateReceived1">
        		<title>Date Received</title>
        		<description>Date Received</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>

<type name="empfm:pf2">
            <title>Provident Fund Form 2</title>
            <parent>empfm:empdoc</parent>
            <properties>
               <property name="empfm:DateReceived">
        		<title>Date Received</title>
        		<description>Date Received</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>


<type name="empfm:pf10">
            <title>Provident Fund Form 10</title>
            <parent>empfm:empdoc</parent>
            <properties>
               <property name="empfm:datefiled">
        		<title>Date Filed</title>
        		<description>Date Filed</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>

<type name="empfm:esicform">
            <title>ESIC FORM</title>
            <parent>empfm:empdoc</parent>
            <properties>
               <property name="empfm:esicno">
        		<title>ESIC NUMBER</title>
        		<description>ESIC NUMBER</description>
        		<type>d:text</type>
        	</property>
		<property name="empfm:nominees">
        		<title>NAME OF NOMINEES</title>
        		<description>NAME OF NOMINEES</description>
        		<type>d:text</type>
        	</property>
            </properties>
        </type>

     <type name="empfm:empfile">
        <title>Employee File</title>
        <parent>cm:folder</parent>
          	 <properties>
        	<property name="empfm:EmployeeFile">
        		<title>Employee Name</title>
        		<description>Name of the microarray project</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="empfm:EmployeeId1">
        		<title>Employee Id</title>
        		<description>Employee Id</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="empfm:DeptName">
        		<title>Department Name</title>
        		<description>Department Name</description>
        		<type>d:text</type>
        		
        	</property>
              	<property name="empfm:MgrName">
        		<title>Manager Name</title>
        		<description>Manager Name</description>
        		<type>d:text</type>
        		
        	</property>
             	<property name="empfm:MgrId">
        	        <title>Manager ID</title>
                       <description>Manager ID</description>
                       <type>d:text</type>
                       
                </property>
		<property name="empfm:DOJ">
        	        <title>Date of Joining</title>
                       <description>Date of Joining</description>
                       <type>d:date</type>
                       
                </property>
		<property name="empfm:DOL">
        	        <title>Date of Leaving</title>
                       <description>Date of Leaving</description>
                       <type>d:date</type>
                       
                </property>
		<property name="empfm:UFI">
        	        <title>Unique File Identifier</title>
                       <description>Unique File Identifier</description>
                       <type>d:text</type>                       
                </property>
		<property name="empfm:alltypes">
        		<title>All Types</title>
        		<description>Update count when all types are present</description>
        		<type>d:int</type> 
			<default>1</default>       		
        	</property>

		<property name="empfm:alltypetext">
        		<title>Types Present</title>
        		<description>Types Present in folder</description>
        		<type>d:text</type>        	
			<default>defaultValue</default>	
        	</property>
          </properties>
     </type>
   </types>
</model>