<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="cm:empfilemodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <!-- Optional meta-data about the model -->
    <description>Employee File Model</description>
  <author>Shikha</author>
  <version>1.0</version>
  <imports>
     <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
     <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
  </imports>
   <!-- Introduction of new namespaces defined by this model -->
    <namespaces>
        <namespace uri="http://www.someco.com/model/content/1.0" prefix="sc" />
    </namespaces>
  <types>   
 <type name="sc:empdoc">
            <title>Employee Document</title>
            <parent>cm:content</parent>
            <properties>
        	<property name="sc:EmployeeName">
        		<title>Employee Name</title>
        		<description>Name of the microarray project</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="sc:EmployeeId">
        		<title>Employee Id</title>
        		<description>Employee Id</description>
        		<type>d:text</type>
        	</property>
        	<property name="sc:UID">
        		<title>Unique File Identifier</title>
        		<description>Department Name</description>
        		<type>d:text</type>
                </property>              	
          </properties>
        </type>

 <type name="sc:resume">
            <title>Resume</title>
            <parent>sc:empdoc</parent>
            <properties>
               <property name="sc:DateReceived1">
        		<title>Date Received</title>
        		<description>Date Received</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>

<type name="sc:pf2">
            <title>Provident Fund Form 2</title>
            <parent>sc:empdoc</parent>
            <properties>
               <property name="sc:DateReceived">
        		<title>Date Received</title>
        		<description>Date Received</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>


<type name="sc:pf10">
            <title>Provident Fund Form 10</title>
            <parent>sc:empdoc</parent>
            <properties>
               <property name="sc:datefiled">
        		<title>Date Filed</title>
        		<description>Date Filed</description>
        		<type>d:date</type>
        	</property>
            </properties>
        </type>

<type name="sc:esicform">
            <title>ESIC FORM</title>
            <parent>sc:empdoc</parent>
            <properties>
               <property name="sc:esicno">
        		<title>ESIC NUMBER</title>
        		<description>ESIC NUMBER</description>
        		<type>d:text</type>
        	</property>
		<property name="sc:nominees">
        		<title>NAME OF NOMINEES</title>
        		<description>NAME OF NOMINEES</description>
        		<type>d:text</type>
        	</property>
            </properties>
        </type>

     <type name="sc:empfile">
        <title>Employee File</title>
        <parent>cm:folder</parent>
          	 <properties>
        	<property name="sc:EmployeeFile">
        		<title>Employee Name</title>
        		<description>Name of the microarray project</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="sc:EmployeeId1">
        		<title>Employee Id</title>
        		<description>Employee Id</description>
        		<type>d:text</type>
        		
        	</property>
        	<property name="sc:DeptName">
        		<title>Department Name</title>
        		<description>Department Name</description>
        		<type>d:text</type>
        		
        	</property>
              	<property name="sc:MgrName">
        		<title>Manager Name</title>
        		<description>Manager Name</description>
        		<type>d:text</type>
        		
        	</property>
             	<property name="sc:MgrId">
        	        <title>Manager ID</title>
                       <description>Manager ID</description>
                       <type>d:text</type>
                       
                </property>
		<property name="sc:DOJ">
        	        <title>Date of Joining</title>
                       <description>Date of Joining</description>
                       <type>d:date</type>
                       
                </property>
		<property name="sc:DOL">
        	        <title>Date of Leaving</title>
                       <description>Date of Leaving</description>
                       <type>d:date</type>
                       
                </property>
		<property name="sc:UFI">
        	        <title>Unique File Identifier</title>
                       <description>Unique File Identifier</description>
                       <type>d:text</type>
                       
                </property>
          </properties>
     </type>
   </types>
</model>